<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <!-- Fontawesome CSS -->
    <link href="~/Content/vendor/fontawesome/css/all.min.css" rel="stylesheet">

    <!-- Bootstrap core CSS -->
    <link href="~/Content/vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet">

    <!-- Custom styles for this template -->
    <link href="~/Content/vendor/bootstrap/css/starter-template.css" rel="stylesheet">

    <title>@Page.Title</title>
    @RenderSection("head", required: false)
    @RenderSection("styles", false)
</head>
<body>
    <nav class="navbar navbar-expand-md navbar-dark bg-dark fixed-top">
        <a class="navbar-brand" href="#">Lista de tarefas</a>
    </nav>

    <main role="main" class="container">

        <div class="starter-template">
            @RenderBody()
        </div>

    </main><!-- /.container -->
    <!-- Bootstrap core JavaScript
    ================================================== -->
    <!-- Placed at the end of the document so the pages load faster -->
    <script src="~/Content/vendor/jquery/js/jquery-3.3.1.slim.min.js"></script>
    <script>window.jQuery || document.write('<script src="~/Content/vendor/jquery/js/jquery-slim.min.js"><\/script>')</script>
    <script src="~/Content/vendor/popper/js/popper.min.js"></script>
    <script src="~/Content/vendor/bootstrap/js/bootstrap.min.js"></script>
    @RenderSection("scripts", false)
</body>
</html>
